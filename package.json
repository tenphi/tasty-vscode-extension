{
  "name": "tasty-syntax-highlighting",
  "displayName": "Tasty Syntax Highlighting",
  "description": "Syntax highlighting for Tasty CSS-in-JS styles in TSX files",
  "version": "1.0.0",
  "publisher": "cube-dev",
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=20.18.1"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "keywords": [
    "tasty",
    "css-in-js",
    "syntax-highlighting",
    "react",
    "tsx"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/tasty.tmLanguage.json",
        "scopeName": "source.tasty",
        "injectTo": [
          "source.tsx",
          "source.ts",
          "meta.embedded.expression.tsx"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.tasty": "tasty"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:grammar": "node test-grammar.js",
    "test:patterns": "node test-patterns.js",
    "test:all": "npm run test:patterns && npm run test:grammar && node test-injection.js && node test-direct-jsx.js",
    "test:injection": "node test-injection.js",
    "test:direct": "node test-direct-jsx.js",
    "build:vsix": "vsce package",
    "build:manual": "node build-manual.js"
  },
  "devDependencies": {
    "@types/node": "^20.18.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "@vscode/vsce": "^3.2.0",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.git"
  },
  "license": "MIT"
}
